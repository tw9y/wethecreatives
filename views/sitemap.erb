<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>http://wethecreatives.com</loc>
    <priority>1.0</priority>
  </url>
  <url>
    <loc>http://wethecreatives.com/archive</loc>
    <priority>1.0</priority>
  </url>
  <% for post in @posts %>
    <url>
      <loc>http://wethecreatives.com/<%= post.slug %></loc>
      <priority>0.8</priority>
    </url>
  <% end %>
</urlset>